<h1 class="pb-4">Explore-Latest</h1>
<nav aria-label="breadcrumb ">
    <ol class="breadcrumb mt-4">
      <li class="breadcrumb-item mb-2"><a href="/">Home</a></li>
      <li class="breadcrumb-item active" aria-current="page">Explore-latest</li>
    </ol>
  </nav>
<div class=" row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
    <% if(typeof recipe !== 'undefined' && recipe.length > 0){ %>
     <% recipe.forEach(function(recipe,index){ %>
     <a href="/recipe/<%= recipe._id %>" class="col text-center category__link">
       <div class="category__img  category__img--large shadow">
         <img src="uploads/<%= recipe.image %>" alt="<%= recipe.name %>" loading="lazy">
       </div>
       <div class="pt-1">Contact: <%= recipe.phonenumber %></div>
     </a>
     <% }) %>
     <% } else{ %>
       <p>No Items Found</p>
     <% } %>
   </div>